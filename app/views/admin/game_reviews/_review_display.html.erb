<script>
  document.addEventListener('turbolinks:load', function() {
    let raty_elem_<%= game_review.id %> = document.querySelector("#review_display<%= game_review.id %>");
    raty_elem_<%= game_review.id %>.innerHTML = "";
    let raty_opt_<%= game_review.id %> = {
      size: 30,
      starOff: '<%= asset_path('star-off.png') %>',
      starOn: '<%= asset_path('star-on.png') %>',
      starHalf: '<%= asset_path('star-half.png') %>',
      half: true,
      hints: [
        ['1/2 bad', 'bad'],
        ['1/2 poor', 'poor'],
        ['1/2 regular', 'regular'],
        ['1/2 good', 'good'],
        ['1/2 gorgeous', 'gorgeous'],
      ],
      readOnly: true,
      score: '<%= game_review.game_rating %>'
    };
    raty(raty_elem_<%= game_review.id %>,raty_opt_<%= game_review.id %>);
  });
</script>